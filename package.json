{
  "name": "d4s",
  "version": "1.0.0",
  "private": true,
  "description": "D&D Dice Distribution Statistics",
  "keywords": [
    "D&D",
    "M.A.G.U.S.",
    "dice",
    "roll",
    "svelte",
    "rollup",
    "typescript"
  ],
  "type": "module",
  "main": "src/index.ts",
  "license": "MIT",
  "author": "Gaspar Portik",
  "homepage": "https://github.com/gasparportik/d4s",
  "bugs": {
    "url": "https://github.com/gasparportik/d4s/issues"
  },
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "prepare": "svelte-kit sync || echo ''",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "clean": "rimraf ./build",
    "publish:surge": "npm run build && surge build d4s.surge.sh"
  },
  "devDependencies": {
    "@mdi/js": "^7.4.47",
    "@sveltejs/adapter-static": "^3.0.8",
    "@sveltejs/kit": "^2.16.0",
    "@sveltejs/vite-plugin-svelte": "^5.0.0",
    "@biomejs/biome": "^1.9.4",
    "svelte": "^5.0.0",
    "svelte-check": "^4.0.0",
    "typescript": "^5.0.0",
    "vite": "^6.2.5"
  }
}
